<%- include("handf/header", {bodyclass: "portfolio page"}) %>
	<main role="main">
		<div id="intro-wrap" data-height="22.222">
			<div id="intro" class="preload darken">
				<div class="caption">
					<h2>Curriculum</h2>
					<p>The universe is made of stories, not of atoms.</p>
				</div><!-- caption -->
			</div><!-- intro -->
		</div><!-- intro -->

		<div class=" row" id="main">
			<section class="row section" style="background-color: #332219;">
				<div class="row-content buffer even clear-after">
					<div class="section-title">
						<h3 style="color: white; border-bottom-color: white">INTRODUCTION TO LAW</h3>
					</div>
					<div id="main" class="row" style="margin-bottom: 10px;">
						<div style="background-color: #332219;" class="row-content buffer clear-after">
							<ul class="inline cats filter-options">
								<li data-group="CS">Case Study</li>
								<li data-group="CSY">Court System</li>
								<li data-group="CP">Club Presentation</li>
								<li data-group="KT">Key Terms</li>
							</ul>
							<div class="grid-items blog-section masonry-style lightbox preload">
							<% content.forEach((item) => { %>
								<article class="item column three" data-groups=<%= item.tag %>>
									<a target="__blank" rel="noreferrer noopener" href=<%= item.link %>>
										<figure><iframe src= "https://docs.google.com/viewer?url=https://docs.google.com/document/d/15zcI80sEK3KMkWbcBpwWAmZkvu2j0Efezncya5Bdl-Y/edit?usp=sharing&embedded=true" width="600"  height="780" style="border: none;"></iframe>
											<span class="blog-overlay"><i class="fa-regular fa-file-lines"></i></span></figure>
										<div class="blog-excerpt">
											<div class="blog-excerpt-inner">
												<h5 class="meta-post"><%= item.tagFF%></h5>
												<h2><%= item.title %></h2>
											</div><!-- blog-excerpt-inner -->
										</div><!-- blog-excerpt -->
									</a>
								</article>
						<%  }); %> 

								<div class="shuffle-sizer three"></div>
							</div>
						</div>
					</div>

			</section>
		</div>


		<%- include("handf/footer") %>

